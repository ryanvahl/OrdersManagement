    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Fonts\Bahiana-Regular.ttf" />
    <None Remove="Images\boxes.png" />
