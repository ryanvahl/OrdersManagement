    <None Remove="Images\info.png" />
    <None Remove="Images\star.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Images\star.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Fonts\Bahiana-Regular.ttf" />
    <Resource Include="Images\boxes.png" />
